{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adflocalsqltopp"
		},
		"FileServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FileServer1'"
		},
		"mylocalsql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'mylocalsql'"
		},
		"myppDemoDataverse_servicePrincipalCredential": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalCredential' of 'myppDemoDataverse'"
		},
		"FileServer1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\\\192.168.5.50\\home\\csv"
		},
		"FileServer1_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "metalis"
		},
		"mylocalsql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "ROG13600"
		},
		"mylocalsql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "testDB"
		},
		"mylocalsql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa"
		},
		"myppDemoDataverse_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "703009a7-bd48-4e07-b952-480e2f806dea"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adflocalsqltopp_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"myppDemoDataverse_servicePrincipalCredential": {
						"value": "[parameters('myppDemoDataverse_servicePrincipalCredential')]"
					},
					"FileServer1_password": {
						"value": "[parameters('FileServer1_password')]"
					},
					"mylocalsql_password": {
						"value": "[parameters('mylocalsql_password')]"
					},
					"myppDemoDataverse_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('myppDemoDataverse_properties_typeProperties_servicePrincipalId')]"
					},
					"FileServer1_properties_typeProperties_host": {
						"value": "[parameters('FileServer1_properties_typeProperties_host')]"
					},
					"FileServer1_properties_typeProperties_userId": {
						"value": "[parameters('FileServer1_properties_typeProperties_userId')]"
					},
					"mylocalsql_properties_typeProperties_server": {
						"value": "[parameters('mylocalsql_properties_typeProperties_server')]"
					},
					"mylocalsql_properties_typeProperties_database": {
						"value": "[parameters('mylocalsql_properties_typeProperties_database')]"
					},
					"mylocalsql_properties_typeProperties_userName": {
						"value": "[parameters('mylocalsql_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}